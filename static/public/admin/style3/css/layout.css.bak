.menu {
    padding-bottom: 8px;
    border-top: 1px solid #d1d1d1;
   font-size:14px;
}

.menu_title {
    position: relative;
    padding: 0 16px;
    background-color: #f3f3f3;
    width: auto;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    cursor: pointer;
    line-height: 40px;
    box-shadow: inset 0 1px 0 0 #fdfdfd;
    -moz-box-shadow: inset 0 1px 0 0 #fdfdfd;
    -webkit-box-shadow: inset 0 1px 0 0 #fdfdfd;
    border-bottom: 1px solid #d1d1d1;
}

.menu.no_extra {
    border-top-width: 0;
}

.menu_item {
    background-color: #f3f3f3;
    line-height: 31px;
}

.menu_item.selected {
    background-color: #c7ad69;
    background-image: -moz-linear-gradient(top, #c7ad69 0, #ab8b47 100%);
    background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#c7ad69), to(#ab8b47));
    background-image: -webkit-linear-gradient(top, #c7ad69 0, #ab8b47 100%);
    background-image: -o-linear-gradient(top, #c7ad69 0, #ab8b47 100%);
    background-image: linear-gradient(to bottom, #c7ad69 0, #ab8b47 100%);
    border-bottom: 1px solid #ab8b47;
    color: #fff;
}
.menu_item.selected a:hover {
    background-color: #c7ad69;
    background-image: -moz-linear-gradient(top, #c7ad69 0, #ab8b47 100%);
    background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#c7ad69), to(#ab8b47));
    background-image: -webkit-linear-gradient(top, #c7ad69 0, #ab8b47 100%);
    background-image: -o-linear-gradient(top, #c7ad69 0, #ab8b47 100%);
    background-image: linear-gradient(to bottom, #c7ad69 0, #ab8b47 100%);
    color: #fff;
}

.icon_menu {
    width: 23px;
    height: 23px;
    vertical-align: middle;
    display: inline-block;
    margin-top: -0.2em;
    margin-right: 10px;
}

.icon_menu_switch {
    position: absolute;
    right: 16px;
    top: 50%;
    margin-top: -9px;
}

.icon_menu_switch {
    background: url("../images/base_z.png") 0 -396px no-repeat;
    width: 16px;
    height: 16px;
    vertical-align: middle;
    display: inline-block;
}

.menu_item a {
    display: block;
    padding: 0 36px 0 50px;
    width: auto;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    color: #222;
}

.menu_item.selected a {

    color: #fff;
}

.icon_menu.function {
    background: url("../images/icon1_18.png") no-repeat;
}

.icon_menu.manage {
    background: url("../images/icon2_22.png") no-repeat;
}

.menu_item a:hover {
    background:#DBC58C;
    color:#fff;
    position:relative;
}

.menu_item a.visited {
    background: #DBC58C;
    opacity: .6;
    color: #000;
}

.menu.closed .menu_item {
    display: none;
}

.menu.closed {
    padding-bottom: 0;
}

.closed .icon_menu_switch {
    background: url("../images/base_z.png") 0 -422px no-repeat;
}

.menu.closed .menu_title {
    margin-bottom: 0;
    border-bottom-width: 0;
}

